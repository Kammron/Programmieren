<h1>Listing graphs</h1>

<table>
  <tr>
    <th>Start</th>
    <th>Ende</th>
    <th>Laenge</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @graphs.each do |graph| %>
  <tr>
    <td><%= graph.Start %></td>
    <td><%= graph.Ende %></td>
    <td><%= graph.Laenge %></td>
    <td><%= link_to 'Show', graph %></td>
    <td><%= link_to 'Edit', edit_graph_path(graph) %></td>
    <td><%= link_to 'Destroy', graph, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

